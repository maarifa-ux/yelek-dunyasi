{
  "name": "mobile_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "clean": "cd android && gradlew clean",
    "clean:win": "cd android && .\\gradlew clean",
    "launch": "adb shell am start -n com.mobile_app/com.mobile_app.MainActivity",
    "devices": "adb devices",
    "reload": "adb shell input keyevent 82",
    "reload:full": "adb shell input keyevent 82 && adb shell input keyevent 20 && adb shell input keyevent 23",
    "build:android": "cd android && gradlew assembleDebug",
    "build:android:win": "cd android && .\\gradlew assembleDebug",
    "clear-metro": "npx react-native start --reset-cache",
    "dev-menu": "adb shell input keyevent 82",
    "uninstall": "adb shell pm uninstall com.mobile_app",
    "log:android": "adb logcat *:S ReactNative:V ReactNativeJS:V"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "^5.1.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/datetimepicker": "^8.3.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@react-navigation/stack": "^7.2.10",
    "axios": "^1.9.0",
    "date-fns": "^4.1.0",
    "expo-linear-gradient": "^14.0.2",
    "react": "19.0.0",
    "react-native": "0.79.1",
    "react-native-calendars": "^1.1311.1",
    "react-native-gesture-handler": "^2.25.0",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-vector-icons": "^10.2.0",
    "sharp": "^0.34.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.1",
    "@react-native/eslint-config": "0.79.1",
    "@react-native/metro-config": "0.79.1",
    "@react-native/typescript-config": "^0.79.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.2",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
